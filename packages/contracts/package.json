{
  "name": "@zk-email/ether-email-auth-contracts",
  "version": "0.0.2-preview",
  "license": "MIT",
  "scripts": {
    "build": "forge build --skip '*ZKSync*'",
    "zkbuild": "forge build --zksync",
    "test": "forge test --no-match-test \"testIntegration\" --no-match-contract \".*Script.*\" --skip '*ZKSync*'",
    "zktest": "forge test --no-match-test \"testIntegration\" --no-match-contract \".*Script.*\" --system-mode=true --zksync --gas-limit 1000000000 --chain 300",
    "lint": "solhint 'src/**/*.sol'"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@zk-email/contracts": "^6.1.5",
    "@matterlabs/zksync-contracts": "^0.6.1",
    "solady": "^0.0.123",
    "solidity-stringutils": "github:LayerZero-Labs/solidity-stringutils"
  },
  "devDependencies": {
    "ds-test": "https://github.com/dapphub/ds-test",
    "forge-std": "https://github.com/foundry-rs/forge-std",
    "solhint": "^3.6.1",
    "@zk-jwt/zk-jwt-contracts": "git+https://github.com/zkemail/zk-jwt.git#e71a93f9bd27bc9d5b65330c5e4ccb5231d5710f"
  },
  "files": [
    "/src",
    "foundry.toml",
    "package.json",
    "README.md",
    "remappings.txt"
  ]
}